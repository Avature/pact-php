{
    "name": "mattersight/phppact",
    "keywords": [
        "mattersight",
        "pact",
        "pact-php",
        "pact-php-native",
        "php-mock"
    ],
    "homepage": "https://github.com/Mattersight/php-pact",
    "description": "Enables consumer driven contract testing, following the PACT foundation principles.",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Mattermack",
            "email": "charles.mack@mattersight.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mattermack/JsonPath"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-curl": "*",
        "php-http/guzzle6-adapter": "^1.1",
        "windwalker/http": "~3.0",
        "apache/log4php": "^2.3",
        "jfalque/http-mock": "^1.0",
        "justinrainbow/json-schema": "5.2.1",
        "lukascivil/treewalker": "dev-master",
        "peekmo/jsonpath": "dev-feature_pathing_retention",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-xml2json": "^3.1",
        "symfony/process": "^3.0",
        "symfony/filesystem": "^3.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "friendsofphp/php-cs-fixer": "^2.10",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PhpPact\\": "src/PhpPact"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpPactTest\\": "tests/PhpPact"
        }
    },
    "scripts": {
        "test": [
            "php-cs-fixer fix --config .php_cs",
            "phpunit --configuration=phpunit-all-tests.xml"
        ]
    }
}
